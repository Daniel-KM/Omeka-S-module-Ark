{
    "name": "daniel-km/omeka-s-module-ark",
    "description": "Module for Omeka S to create short, unique, meaningless, universal and persistent identifiers (ark and/or noid) and use them as urls for records.",
    "type": "library",
    "license": "CECILL-2.1",
    "authors": [
        {
            "name": "Daniel Berthereau",
            "email": "Daniel.packagist@Berthereau.net"
        }
    ],
    "homepage": "https://gitlab.com/Daniel-KM/Omeka-S-module-Ark",
    "support": {
        "issues": "https://gitlab.com/Daniel-KM/Omeka-S-module-Ark/-/issues",
        "forum": "https://forum.omeka.org/"
    },
    "keywords": [
        "Omeka S",
        "module",
        "identifier",
        "utility",
        "nice opaque identifier",
        "persistent identifier",
        "digital object",
        "noid"
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "daniel-km/noid": "^1.4"
    },
    "suggest": {
        "ext-dba": "Required for LMDB (default) or BerkeleyDB storage backend",
        "ext-pdo": "Required for PDO storage backend (MySQL, PostgreSQL, SQLite)",
        "ext-sqlite3": "Required for SQLite3 storage backend",
        "ext-simplexml": "Required for XML storage backend"
    },
    "autoload": {
        "psr-4": {
            "Ark\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ArkTest\\": "tests/ArkTest/"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    },
    "extra": {
        "installer-name": "Ark"
    }
}
